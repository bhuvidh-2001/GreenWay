<%- include('./partials/header') %>
<div class="blog">
	<header>
  <h1>Company :</h1>
  <a href="/company/new">Post A Project</a>
  
</header>
  
<div class="band">


        
    <% for(let i=0 ;i< company.length ;i++){ %>
        <div class = "item-2" >
            <a href = 'mailto:<%= company[i].email %>' class = "card" > 
            <div class = "thumb" style = "background-image: url('<%= company[i].img %>'); " > </div> 
        
            <article >
            <p > <strong>Name: </strong><%= company[i].Name %> </p>  
            <p > <strong>Company Type:</strong><%= company[i].CompanyType %> </p>
            <p > <strong>Crop:</strong><%= company[i].Crop %> </p>
            <p > <strong> Price(per quintol): </strong><%= company[i].Price %> </p>
            
            <p > <strong>Contract Type:</strong><%= company[i].ContractType %> </p>
           
            
            <p ><strong> CIN:</strong> <%= company[i].Cin %> </p>
             <p > <strong>Address:</strong><br><%= company[i].Address %> </p> 
            <p ><strong> Email:</strong><%= company[i].email %> </p>
            <!-- <p ><strong> Email:</strong><a href="mailto:Test@gmail.com">click</a> </p> -->
            
                <p ><strong>Contact No.: </strong><%= company[i].contactNo %> </p>
          
            </article> 
            </a> 
          </div>
          <% } %>
</div>
</div>


<%- include('./partials/footer') %>
